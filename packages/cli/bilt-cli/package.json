{
  "name": "@bilt/bilt-cli",
  "version": "1.1.7",
  "description": "The CLI that builds a local folder",
  "main": "src/bilt-cli.js",
  "engines": {
    "node": ">=8.3.0"
  },
  "files": [
    "src",
    "scripts"
  ],
  "bin": {
    "bilt": "scripts/run-bilt-cli.js"
  },
  "scripts": {
    "test": "npm run test:mocha && npm run eslint",
    "test:mocha": "mocha --no-timeouts 'test/**/*.test.js'",
    "eslint": "eslint '**/*.js'",
    "build": "npm run build:e2e-keys && npm run build:e2e-git-repo",
    "build:e2e-keys": "echo y | ssh-keygen -t rsa -P '' -f test/it/bilt-cli/git-server/keys/id_rsa",
    "build:e2e-git-repo": "git init --bare test/it/bilt-cli/git-server/repos/test-repo"
  },
  "keywords": [
    "ci",
    "build"
  ],
  "author": "Gil Tayar <gil@tayar.org>",
  "license": "Unlicense",
  "repository": {
    "url": "https://github.com/giltayar/bilt",
    "type": "git"
  },
  "dependencies": {
    "@bilt/artifact-finder": "^1.0.23",
    "@bilt/git-commander": "^1.0.5",
    "@bilt/host-agent": "^1.0.13",
    "@bilt/in-memory-events": "^1.0.7",
    "@bilt/in-memory-job-dispatcher": "^1.0.7",
    "@bilt/jobs": "^1.0.9",
    "@bilt/last-build-info": "^2.0.1",
    "@bilt/local-docker-agent": "^1.0.8",
    "@bilt/noop-repository-fetcher": "^1.0.7",
    "@bilt/npm-build-job": "^1.0.12",
    "@bilt/npm-commander": "^1.0.5",
    "@bilt/repo-build-job": "^1.1.4",
    "@bilt/symlink": "^1.0.7",
    "cosmiconfig": "^5.0.5",
    "debug": "^3.1.0",
    "git-repo-info": "^2.0.0",
    "ignore": "^4.0.3",
    "jest-changed-files": "^23.4.2",
    "lodash.flatten": "^4.4.0",
    "lodash.pickby": "^4.6.0",
    "plugin-import": "^1.0.14",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "cpr": "^3.0.1",
    "docker-compose-mocha": "^1.2.0",
    "eslint": "^5.2.0",
    "eslint-plugin-mocha-no-only": "^1.0.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "npm-registry-client": "^8.6.0",
    "prettier": "^1.14.0",
    "test-fixture": "^1.0.2"
  }
}
